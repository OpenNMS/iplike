#!/bin/sh
# prerm script for iplike-pgsql
#
# see: dh_installdeb(1)

set -e

POSTGRESQL_VERSION="@POSTGRESQL_VERSION@"
PGSQL_TYPE="@PGSQL_TYPE@"
PG_LIBDIR="@PG_LIBDIR@"

case "$1" in
    remove)
        update-alternatives --remove "iplike.so"         "${PG_LIBDIR}/iplike.so"
        update-alternatives --remove "install_iplike.sh" "/usr/sbin/install_iplike-${PGSQL_TYPE}.sh"
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0


