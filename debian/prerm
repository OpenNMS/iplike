#!/bin/sh
# prerm script for iplike-pgsql84
#
# see: dh_installdeb(1)

set -e

case "$1" in
    remove)
        /usr/sbin/update-alternatives --remove "iplike.so"         "/usr/lib/postgresql/8.4/lib/iplike.so"
        /usr/sbin/update-alternatives --remove "install_iplike.sh" "/usr/sbin/install_iplike-84.sh"
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0


